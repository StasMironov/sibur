{
	"form": {
		"id": "form-1",
		"name": "form-1",
		"method": "POST",
		"action": "/ajax/contacts/?repo=atol-education.frontend&status=success",
		"attr": "data-ajax-form",
		"title_hidden": "Оптимальные решения для партнеров",
		"title": [
			"Оставьте",
			"<span>заявку</span>"
		],
		"fields": [
			{
				"input": true,
				"id": "form-1-input-1",
				"name": "form-1-input-1",
				"required": true,
				"label": "Имя"
			},
			{
				"input": true,
				"id": "form-1-input-2",
				"name": "form-1-input-2",
				"required": true,
				"label": "E-mail"
			},
			{
				"input": true,
				"id": "form-1-input-3",
				"name": "form-1-input-3",
				"required": true,
				"type": "tel",
				"label": "Телефон",
				"mask_list": [
					"+998-##-###-####",
					"+7(###)###-##-##",
					"+380(##)###-##-##",
					"+7(6##)###-##-##",
					"+992-##-###-####",
					"+994-##-###-##-##",
					"+996-###-######"
				]
			},
			{
				"textarea": true,
				"id": "form-1-textarea-1",
				"name": "form-1-textarea-1",
				"label": "Сообщение"
			}
		],
		"background": {
			"src": "tmp/form/bg.jpg",
			"alt": "alt image"
		},
		"agreement": "Нажимая на кнопку, я соглашаюсь с&nbsp;<a class='link' href='#'>политикой обработки персональных данных</a>",
		"submit": {
			"type": "submit",
			"text": "Отправить заявку",
			"title": "Отправить заявку",
			"theme": "green"
		}
	}
}
